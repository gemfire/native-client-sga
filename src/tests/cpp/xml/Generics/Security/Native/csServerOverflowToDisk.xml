<?xml version="1.0"?>
<cache xmlns="http://geode.apache.org/schema/cache"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://geode.apache.org/schema/cache http://geode.apache.org/schema/cache/cache-1.0.xsd"
       version="1.0">
  <cache-server port="$PORT_NUM" maximum-message-count="50000"/>
  <region name="TestTCR1">
    <region-attributes scope="distributed-ack" data-policy="replicate" persist-backup="true">
      <eviction-attributes>
        <lru-heap-percentage maximum="70" time-interval="100" action="overflow-to-disk"/>
      </eviction-attributes>
    </region-attributes>
  </region>
</cache> 
